{
  "description": "TypeRocket Plugin",
  "name": "maker-maker/makermaker",
  "require": {
    "php": ">=7.2",
    "doctrine/inflector": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "pestphp/pest": "^2.34",
    "brain/monkey": "^2.6",
    "mockery/mockery": "^1.6"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MakerMaker\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MakerMaker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "pest",
    "test:unit": "pest --group=unit",
    "test:quick": "pest --group=unit,smoke --testdox",
    "test:ci": "pest --coverage --min=85",
    "test:all": "pest --testsuite=Unit,Integration,Feature,Acceptance",
    "test:affected": "pest --group!=slow,quarantine"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}